@model BackOffice.Models.DisqueraModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

@using (Html.BeginForm("AddDisquera", "Disco", FormMethod.Post))
{
    @Html.ValidationSummary(true, "No se pudo crear la disquera.")
    <div>
        <fieldset>
            <legend>Agregar Disquera</legend>
            <div class="editor-label">
                @Html.LabelFor(m => m.Ndisquera)
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(m => m.Ndisquera)
                @Html.ValidationMessageFor(m => m.Ndisquera)
            </div>
            <p>
                <input type="submit" value="Agregar" />
            </p>
        </fieldset>
    </div>
}
